{
  "buildOptions": {
    "outDir": "./tools",
    "rootDir": "./tools"
  },
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "NodeNext",
    "outDir": "./tools/build",
    "pretty": true,
    "target": "ESNext"
  },
  "exclude": [
    "./node_modules"
  ],
  "extends": "./tsconfig.json",
  "files": [
    "./src/build/index.ts"
  ],
  "include": [
    "./node_modules/sass/types/*.ts"
  ]
}
